{
  "name": "71 Lines",
  "version": "1.0",
  "description": "Build an Extension with Bubble",
  "permissions": ["activeTab", "declarativeContent", "tabs"],

  "background": {
    "scripts": ["background.js", "popup.js"],
    "persistent": false
  },

  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/71_16.png",
      "32": "images/71_32.png",
      "48": "images/71_48.png",
      "128": "images/71_128.png"
    }
  },

  "icons": {
    "16": "images/71_16.png",
    "32": "images/71_32.png",
    "48": "images/71_48.png",
    "128": "images/71_128.png"
  },

  "manifest_version": 2
}